{
  "manifest_version": 3,
  "name": "URTab",
  "version": "2.0",
  "description": "Customize UR tab's background with any image, GIF, or video.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "assets/icon.png"
  },
  "action": {
    "default_popup": "newtab.html",
    "default_icon": "assets/settingsicon.png"
  }
}
